{
  "name": "zalla-io-website",
  "version": "1.0.0",
  "description": "The personal website of Michael Zalla.",
  "main": "package.json",
  "scripts": {
    "postinstall": "./postinstall.sh",
    "build": "hugo --verbose --minify",
    "serve": "hugo server -w --noHTTPCache --disableFastRender -v -p 1313",
    "serve:draft": "hugo server -w --noHTTPCache --disableFastRender -v -p 1313 -D",
    "deploy": "aws s3 sync --delete ./public s3://zalla.io --exclude=\"./demos/*\" --profile zalla-io-s3-manager",
    "deploy-html": "aws s3 sync ./public s3://zalla.io --exclude \"*\" --include \"*.html\" --profile zalla-io-s3-manager",
    "deploy-css": "aws s3 sync ./public s3://zalla.io --exclude \"*\" --include \"*.css\" --profile zalla-io-s3-manager",
    "deploy-js": "aws s3 sync ./public s3://zalla.io --exclude \"*\" --include \"*.js\" --profile zalla-io-s3-manager",
    "deploy-fonts": "aws s3 sync ./public s3://zalla.io --exclude \"*\" --include \"*fonts*\" --profile zalla-io-s3-manager",
    "deploy-google": "aws s3 sync ./public s3://zalla.io/ --exclude \"*\" --include \"googlece1068047507cc18.html\" --profile zalla-io-s3-manager",
    "deploy:qa": "npm run deploy:qa-html && npm run deploy:qa-css && npm run deploy:qa-js",
    "deploy:qa-assets": "aws s3 sync --delete ./public s3://zalla.io-qa --exclude=\"*\" --include=\"*.png\" --include=\"*.svg\" --include=\"*.pdf\" --exclude=\"/demos/\" --profile zalla-io-s3-manager",
    "deploy:qa-html": "aws s3 sync ./public s3://zalla.io-qa --exclude=\"*\" --include \"*.html\" --exclude=\"/demos/\" --profile zalla-io-s3-manager",
    "deploy:qa-css": "aws s3 sync ./public s3://zalla.io-qa --exclude=\"*\" --include \"*.css\" --exclude=\"/demos/\" --profile zalla-io-s3-manager",
    "deploy:qa-js": "aws s3 sync ./public s3://zalla.io-qa --exclude=\"*\" --include \"*.js\" --exclude=\"/demos/\" --profile zalla-io-s3-manager",
    "deploy:qa-fonts": "aws s3 sync ./public s3://zalla.io-qa --exclude=\"*\" --include \"*fonts*\" --exclude=\"/demos/\" --profile zalla-io-s3-manager",
    "deploy:dry": "aws s3 sync --delete --dryrun ./public s3://zalla.io --exclude=\"/demos/\" --profile zalla-io-s3-manager",
    "clean": "rm -rf ./public ./public_min"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MichaelZalla/zalla-io-website.git"
  },
  "author": "Michael Zalla (https://zalla.io)",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/MichaelZalla/zalla-io-website/issues"
  },
  "homepage": "https://github.com/MichaelZalla/zalla-io-website#readme",
  "devDependencies": {
    "autoprefixer": "^10.4.0",
    "postcss-cli": "^9.0.2",
    "sass": "^1.26.8",
    "sass-breakpoints-mixins": "github:MichaelZalla/sass-breakpoints-mixins"
  },
  "dependencies": {
    "disqusLoader.js": "git+https://github.com/osvaldasvalutis/disqusLoader.js.git",
    "insert-css": "^0.0.0",
    "jquery": "^3.5.1",
    "vanilla-lazyload": "^17.1.0"
  }
}
