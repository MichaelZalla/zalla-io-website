{
  "name": "zalla-io-website",
  "version": "1.0.0",
  "description": "The personal website of Michael Zalla.",
  "main": "package.json",
  "scripts": {
    "postinstall": "./postinstall.sh",
    "build": "hugo --verbose --minify",
    "serve": "hugo server -w --noHTTPCache --disableFastRender -v -p 1313",
    "serve:draft": "hugo server -w --noHTTPCache --disableFastRender -v -p 1313 -D",
    "deploy": "aws s3 sync --delete ./public s3://zalla.io --exclude=\"projects/*/images/*\" --exclude=\"projects/*/attachments/*\" --exclude=\"./demos/*\" --profile $AWS_PROFILE",
    "deploy-html": "aws s3 sync ./public s3://zalla.io --exclude \"*\" --include \"*.html\" --include \"*.xml\" --profile $AWS_PROFILE",
    "deploy-css": "aws s3 sync ./public s3://zalla.io --exclude \"*\" --include \"*.css\" --profile $AWS_PROFILE",
    "deploy-js": "aws s3 sync ./public s3://zalla.io --exclude \"*\" --include \"*.js\" --profile $AWS_PROFILE",
    "deploy-fonts": "aws s3 sync ./public s3://zalla.io --exclude \"*\" --include \"*fonts*\" --profile $AWS_PROFILE",
    "deploy-google": "aws s3 sync ./public s3://zalla.io/ --exclude \"*\" --include \"googlece1068047507cc18.html\" --profile $AWS_PROFILE",
    "deploy:qa": "npm run deploy:qa-html && npm run deploy:qa-css && npm run deploy:qa-js",
    "deploy:qa-assets": "aws s3 sync --delete ./public s3://zalla.io-qa --exclude=\"*\" --include=\"*.png\" --include=\"*.svg\" --include=\"*.pdf\" --exclude=\"/demos/\" --profile $AWS_PROFILE",
    "deploy:qa-html": "aws s3 sync ./public s3://zalla.io-qa --exclude=\"*\" --include \"*.html\" --exclude=\"/demos/\" --profile $AWS_PROFILE",
    "deploy:qa-css": "aws s3 sync ./public s3://zalla.io-qa --exclude=\"*\" --include \"*.css\" --exclude=\"/demos/\" --profile $AWS_PROFILE",
    "deploy:qa-js": "aws s3 sync ./public s3://zalla.io-qa --exclude=\"*\" --include \"*.js\" --exclude=\"/demos/\" --profile $AWS_PROFILE",
    "deploy:qa-fonts": "aws s3 sync ./public s3://zalla.io-qa --exclude=\"*\" --include \"*fonts*\" --exclude=\"/demos/\" --profile $AWS_PROFILE",
    "deploy:dry": "aws s3 sync --delete --dryrun ./public s3://zalla.io --exclude=\"/demos/\" --profile $AWS_PROFILE",
    "monitor:delete-logs": "mkdir -p empty aws s3 sync --delete ./empty s3://zalla.io-log --profile $AWS_PROFILE",
    "open:qa": "open http://zalla.io-qa.s3-website-us-west-1.amazonaws.com",
    "open:analytics": "open https://analytics.google.com/analytics/web/#/report-home/a51749284w84070439p87107294",
    "open:crawler": "open https://search.google.com/search-console/inspect?resource_id=https%3A%2F%2Fzalla.io%2F&id=dQtNmtMOdzPOELz0whUioA",
    "open:s3": "open https://s3.console.aws.amazon.com/s3/buckets/zalla.io-qa?region=us-west-1&tab=objects",
    "open:cloudfront": "open https://console.aws.amazon.com/cloudfront/v3/home?region=us-west-1#/distributions/E8D1TC5IEFS1D",
    "clean": "rm -rf ./public ./public_min ./.hugo_build.lock"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MichaelZalla/zalla-io-website.git"
  },
  "author": "Michael Zalla (https://zalla.io)",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/MichaelZalla/zalla-io-website/issues"
  },
  "homepage": "https://github.com/MichaelZalla/zalla-io-website#readme",
  "devDependencies": {
    "autoprefixer": "^10.4.0",
    "postcss": "^8.3.11",
    "postcss-cli": "^9.0.2",
    "sass": "^1.26.8",
    "sass-breakpoints-mixins": "github:MichaelZalla/sass-breakpoints-mixins"
  },
  "dependencies": {
    "disqusLoader.js": "git+https://github.com/osvaldasvalutis/disqusLoader.js.git",
    "insert-css": "^0.0.0",
    "vanilla-lazyload": "^17.1.0",
    "vanilla-tilt": "^1.7.2"
  }
}
